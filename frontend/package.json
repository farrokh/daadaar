{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "bun scripts/generate-updates.ts && next build",
    "start": "next start",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "type-check": "tsc --noEmit",
    "test": "echo 'No tests configured yet' && exit 0",
    "generate-updates": "bun scripts/generate-updates.ts",
    "deploy:prod": "bun generate-updates && vercel deploy --prod --cwd ./ --yes"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.25.0",
    "lucide-react": "^0.562.0",
    "next": "^16.0.0",
    "next-intl": "^4.7.0",
    "openai": "^6.15.0",
    "posthog-js": "^1.316.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.69.0",
    "reactflow": "^11.11.4",
    "tailwindcss": "^4.1.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@tailwindcss/postcss": "^4.1.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^25.0.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "postcss": "^8.4.33",
    "typescript": "^5.3.3"
  }
